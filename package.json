{
  "name": "api-service",
  "version": "0.0.1",
  "description": "Api service.",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && tsc -p tsconfig.json",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "seed": "cross-env medusa seed -f ./data/seed.json",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks index.js",
    "dev": "cross-env npm run build && medusa develop",
    "build:admin": "cross-env medusa-admin build",
    "migrations": "cross-env medusa migrations run",
    "serve": "cross-env medusa start"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.21.5",
    "@medusajs/cache-inmemory": "^1.8.5",
    "@medusajs/cache-redis": "^1.8.5",
    "@medusajs/event-bus-local": "^1.9.2",
    "@medusajs/event-bus-redis": "^1.8.5",
    "@medusajs/medusa": "^1.10.1",
    "@medusajs/medusa-cli": "^1.3.13",
    "babel-preset-medusa-package": "^1.1.19",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "medusa-fulfillment-manual": "^1.1.37",
    "medusa-interfaces": "^1.3.7",
    "medusa-payment-manual": "^1.0.23",
    "medusa-payment-stripe": "^4.0.1",
    "typeorm": "npm:@medusajs/typeorm@next",
    "medusa-payment-paypal": "^4.0.1",
    "medusa-plugin-algolia": "^0.2.15",
    "medusa-plugin-gcp": "^2.1.1",
    "medusa-plugin-slack-notification": "^1.3.9",
    "medusa-plugin-smtp": "^1.0.15",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.0",
    "babel-preset-medusa-package": "^1.1.19",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "mongoose": "^7.1.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "transform": {
      ".ts": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
